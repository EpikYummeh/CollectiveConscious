<!DOCTYPE html>@Page.IsAsync="true"
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Collective Conscious</title>
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
    <link href='https://fonts.googleapis.com/css?family=Holtwood+One+SC' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Kaushan+Script|Herr+Von+Muellerhoff' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Abel' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Istok+Web|Roboto+Condensed:700' rel='stylesheet' type='text/css'>
    @Styles.Render("~/Content/css")
    @*@Scripts.Render("~/bundles/modernizr")*@

</head>
<body>
    <header class="hero">
        <div class="hero-wrap">
            <p class="intro" id="intro">Destiny 2</p>
            <h1 id="headline">Clan Tournament</h1>
            <p class="year"><i class="fa fa-star"></i> 2018 <i class="fa fa-star"></i></p>
            <p>Collective Conscious</p>
        </div>
    </header>
    @if (TempData["UserMessage"] != null)
    {
        var message = (clanTournament.Models.MessageVM)TempData["UserMessage"];
        <section id="bracket">
            <div class="container">                
                <div class="@message.ClassName">
                    <strong>@message.Title</strong><br />
                    @message.Message
                    <br/>
                    <a href="/Home/Signup/" class="col-form-label-sm">Return to the Signup Page</a>
                </div>
            </div>
        </section>
    }

    @RenderBody()
    <section class="share">
        <div class="share-wrap">
            <a class="share-icon" href="#"><i class="fa fa-discord"></i></a>
        </div>
    </section>


    @*@Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @RenderSection("scripts", required: false)*@
</body>
</html>
